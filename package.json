{
  "name": "@debbl/nrm",
  "type": "module",
  "version": "0.0.6-beta.12",
  "description": "NPM registry manager",
  "author": {
    "name": "Debbl",
    "email": "me@aiwan.run",
    "url": "https://github.com/Debbl/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Debbl/nrm/"
  },
  "bugs": {
    "url": "https://github.com/Debbl/nrm/issues/"
  },
  "keywords": [
    "npm",
    "npm registry",
    "npm registry manager"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "nrm": "./index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "esno ./src/index.ts",
    "build": "unbuild",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "release": "unbuild && bumpp && npm publish"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "ini": "^4.1.1",
    "kolorist": "^1.8.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@debbl/eslint-config-ts": "^0.0.49",
    "@types/ini": "^1.3.31",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.6.0",
    "@types/prompts": "^2.4.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.49.0",
    "esno": "^0.17.0",
    "typescript": "^5.2.2",
    "unbuild": "^1.2.1"
  }
}
